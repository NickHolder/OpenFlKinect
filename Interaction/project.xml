<?xml version="1.0" encoding="utf-8"?>
<project>

	<meta title="Intraction" package="com.kinect.interaction" version="1.0.0" company="kinect" />
	<app main="Main" path="Export" file="Interaction" />

	<window background="#000000" fps="30" />
	<window width="800" height="600" />
	<window orientation="landscape" vsync="false" antialiasing="0"  if="cpp"   hardware="true" />

	<source path="Source" />

	<haxelib name="actuate" />
	<haxelib name="openfl" />
	<include name="OpenFLKinect" path="../OpenFLKinect"></include>
	<include name="KinectGraphics" path="../KinectGraphics/Source"></include>

	<assets path="Assets" rename="assets" exclude="nme.svg" />
	<icon path="Assets/nme.svg" />

	<set name="SHOW_CONSOLE" value="1"/>
	<haxedef name="SHOW_CONSOLE"/>
</project>
