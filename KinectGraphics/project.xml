<?xml version="1.0" encoding="utf-8"?>
<project>
	
	<meta title="Graphics" package="com.kinect.graphics" version="1.0.0" company="kinect" />
	<app main="Main" path="Export" file="Depth" />
	
	<window background="#ffff00" fps="30" />
	<window width="640" height="480" />
	<window orientation="landscape" vsync="false" antialiasing="4" if="cpp" />
	
	<source path="Source" />
	
	
	<haxelib name="openfl" />
	<haxelib name="actuate" />
	<include name="OpenFLKinect" path="../OpenFLKinect"></include>
	
	<assets path="Assets" rename="assets" exclude="nme.svg" />
	<assets path="Assets/img" rename="img" />
	<icon path="Assets/nme.svg" />
	<set name="SHOW_CONSOLE" value="1"/>
	<haxedef name="SHOW_CONSOLE"/>
</project>